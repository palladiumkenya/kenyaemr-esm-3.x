    {
      "component": "paymentHistoryLink",
      "name": "payment-history-link",
      "slot": "billing-dashboard-link-slot"
    },
    {
      "component": "paymentPointsLink",
      "name": "payment-points-link",
      "slot": "billing-dashboard-link-slot"
    },
    {
      "component": "paymentModesLink",
