{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "kenyaemr": "^18.2.0"
  },
  "pages": [],
  "extensions": [
    {
      "name": "clinical-view-section",
      "component": "clinicalViewPatientDashboard",
      "slot": "patient-chart-dashboard-slot"
    },
    {
      "name": "hiv-care-and-treatment-dashboard-group-link",
      "slot": "patient-chart-dashboard-slot",
      "component": "hivCareAndTreatMentSideNavGroup"
    },
    {
      "name": "outpatient-department-dashboard-group-link",
      "slot": "patient-chart-dashboard-slot",
      "component": "outPatientDepartmentSideNavGroup"
    },
    {
      "name": "inpatient-department-dashboard-group-link",
      "slot": "patient-chart-dashboard-slot",
      "component": "inPatientDepartmentSideNavGroup"
    },
    {
      "name": "mch-clinical-view",
      "slot": "patient-chart-mch-dashboard-slot",
      "component": "mchClinicalView",
      "order": 0,
      "online": true,
      "offline": false
    },
    {
      "name": "mch-dashboard-link",
      "component": "mchDashboardLink",
      "slot": "hiv-care-and-treatment-slot",
      "meta": {
        "columns": 1,
        "columnSpan": 1,
        "slot": "patient-chart-mch-dashboard-slot",
        "path": "mch-dashboard",
        "layoutMode": "anchored"
      }
    },
    {
      "name": "hts-clinical-view",
      "slot": "patient-chart-hts-dashboard-slot",
      "component": "htsClinicalView",
      "order": 2,
      "online": true,
      "offline": false
    },
    {
      "name": "hts-dashboard-link",
      "component": "htsDashboardLink",
      "slot": "hiv-care-and-treatment-slot",
      "meta": {
        "columns": 1,
        "columnSpan": 1,
        "slot": "patient-chart-hts-dashboard-slot",
        "path": "hts-dashboard",
        "layoutMode": "anchored"
      }
    },
    {
      "name": "defaulter-tracing-dashboard",
      "slot": "patient-chart-defaulter-tracing-dashboard-slot",
      "component": "defaulterTracing",
      "order": 3,
      "online": true,
      "offline": false
    },
    {
      "name": "defaulter-tracing-dashboard-link",
      "component": "defaulterTracingLink",
      "slot": "hiv-care-and-treatment-slot",
      "meta": {
        "columns": 1,
        "columnSpan": 1,
        "slot": "patient-chart-defaulter-tracing-dashboard-slot",
        "path": "defaulter-tracing-dashboard",
        "layoutMode": "anchored"
      }
    },
    {
      "name": "patient-chart-clinical-encounter-link",
      "component": "clinicalEncounterLink",
      "slot": "in-patient-department-slot",
      "meta": {
        "columns": 1,
        "columnSpan": 1,
        "slot": "patient-chart-clinical-encounter-slot",
        "path": "clinical-encounter",
        "layoutMode": "anchored"
      }
    },
    {
      "name": "patient-chart-clinical-encounter",
      "slot": "patient-chart-clinical-encounter-slot",
      "component": "clinicalEncounter",
      "order": 3,
      "online": true,
      "offline": false
    },
    {
      "name": "family-history",
      "slot": "patient-chart-family-history-slot",
      "component": "familyHistory",
      "order": 0,
      "online": true,
      "offline": false
    },
    {
      "name": "family-history-link",
      "component": "familyHistoryLink",
      "slot": "out-patient-department-slot",
      "meta": {
        "columns": 1,
        "columnSpan": 1,
        "slot": "patient-chart-family-history-slot",
        "path": "family-history",
        "layoutMode": "anchored"
      }
    }
  ]
}
