{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "kenyaemr": "^19.0.0"
  },
  "pages": [
    {
      "component": "adrAssessmentApp",
      "route": "adr-assessment"
    }
  ],
  "workspaces": [

  ],
  "extensions": [
    {
      "name": "adr-assessment-side-nav",
      "slot": "adr-assessment-side-nav-slot",
